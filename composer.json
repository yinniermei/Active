{
    "name": "yinniermei/active",
    "description": "The helper for laravel(4-5) applications to get active class base on current route",
    "authors": [
        {
            "name": "yinniermei",
            "email": "ynem@qq.com"
        }
    ],
    "license": "MIT",
	"require": {
		"php": ">=5.5.9",
		"laravel/framework": "^5.0",
		"orchestra/testbench": "^3.1"
	  },
	"require-dev": {
		"phpunit/phpunit": "~4.0",
		"mockery/mockery": "dev-master",
		"codeclimate/php-test-reporter": "dev-master"
	  },
	"autoload": {
		"psr-4": {
		  "Xii\\Active\\": "src/",
		  "Xii\\ActiveTest\\": "tests/"
		},
		"files": [
		  "src/helpers.php"
		]
	  },
	"minimum-stability": "dev",
	"prefer-stable": true
}
